{
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:7274",
    "DangerousAcceptAnyServerCertificateValidator": true
  },
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/Authorization/authorize/{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7053
        }
      ],
      "UpstreamPathTemplate": "/authorize/{everything}",
      "UpstreamHttpMethod": [ "POST" ]
    },
    {
      "DownstreamPathTemplate": "/api/Authorization/authorizeGet/{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7053
        }
      ],
      "UpstreamPathTemplate": "/authorizeGet/{everything}",
      "UpstreamHttpMethod": [ "GET" ]
    },
    {
      "DownstreamPathTemplate": "/api/Confirmation/confirm/{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7079
        }
      ],
      "UpstreamPathTemplate": "/confirm/{everything}",
      "UpstreamHttpMethod": [ "POST" ]
    }
  ]
}
